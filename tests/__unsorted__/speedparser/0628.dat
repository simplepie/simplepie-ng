  &lt;p&gt;&lt;font color=&quot;#000000&quot;&gt;&lt;font face=&quot;Arial, sans-serif&quot;&gt;&lt;font size=&quot;2&quot; style=&quot;font-size: 11pt;&quot;&gt;&lt;b&gt;Screen
2&amp;nbsp;:&lt;/b&gt; (Operations) - Chose &amp;quot;Create a Database&amp;quot; &lt;/font&gt;&lt;/font&gt;&lt;/font&gt; &lt;/p&gt; 
  &lt;p&gt; &lt;/p&gt; 
  &lt;p&gt;&lt;img alt=&quot;&amp;quot;&amp;quot;&quot; src=&quot;/EricBezille/resource/OOW-lab/dbca1.png&quot; style=&quot;width: 657px; height: 533px;&quot; /&gt;&lt;/p&gt; 
